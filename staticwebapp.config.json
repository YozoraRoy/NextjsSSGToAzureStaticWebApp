{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  },
  "routes": [
    {
      "route": "/",
      "rewrite": "/index.html"
    },
    {
      "route": "/:id",
      "rewrite": "/index.html"
    },
    {
      "route": "/api/*",
      "rewrite": "/api/{*}"
    },
    {
      "route": "/API/*",
      "rewrite": "/api/{*}"
    },
    {
      "route": "/api/:id",
      "rewrite": "/api/[id]"
    },
    {
      "route": "/API/:id",
      "rewrite": "/api/[id]"
    }
  ]
}